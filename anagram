#include<bits/stdc++.h>
using namespace std;
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    long long int test;
    cin>>test;
    while(test--){
        string a,b;
        cin>>a>>b;
        long long int h[26]={0};
        for(long long int i=0;i<a.length();i++)h[a[i]-'a']++;
        for(long long int i=0;i<b.length();i++){
            /*if(h[b[i]-'a'])h[b[i]-'a']--;*/
            h[b[i]-'a']--;
        }
        /*for(int i=0;i<26;i++){
            cout<<char(i+'a')<<h[i]<<"\n";
        }*/
        bool flag=true;
        for(int i=0;i<26;i++){
            if(h[i]!=0){
                flag=false;
                break;
            }
        }
        if(flag)cout<<"YES\n";
        else cout<<"NO\n";
    }
    
    
    return 0;
}
