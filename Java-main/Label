import java.util.*;
class BreakLabelDemo{
    public static void main(String[]args){
        boolean t=true;
        first:{
            second:{
                third:{
                    System.out.println("Before break.");
                    if(t)
                    break second;
                    System.out.println("This won't execute");   
                }
                System.out.println("This won't execute");
            }
        }
    }
}
