
import java.io.*;
import java.util.*;

class GFG {
    public static void sum(int []arr){
    int ans=0;
    for(int item:arr){
        ans+=item;
    }
    System.out.println("Sum is "+ans);
}
	public static void main (String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number of elements of array");
		int n=sc.nextInt();
		int []arr= new int[n];
		for(int i=0;i<n;i++){
		    arr[i]=sc.nextInt();
		}
		sum(arr);
	}
}
