#include <iostream>
#include <bits\stdc++.h>
using namespace std;

int main()
{
    string str = "Vansh Punia si DNEGEL";
    stack<char> s;

    for (int i = 0; i < str.length(); i++)
    {
        s.push(str[i]);
    }

    str.clear();

    while (!s.empty())
    {
        str.push_back(s.top());
        s.pop();
    }

    cout << str;
}
