//Program to find difference of "Square of sum of n natural numbers" and "Sum of square of n natural numbers"
#include <stdio.h>
int main(){
    int n;
    int i;
    int count=0;
    int count1=0;
    printf("Enter value of n=");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        count=count+i;
    }
    int final_count=count*count;
    printf("Square of sum of %d natural number is %d\n",n,final_count);

    for(i=1;i<=n;i++){
        count1=count1+i*i;
    }
    int final_count1 = count1;
    printf("Sum of square of %d natural number is %d\n",n,final_count1);

    printf("Difference of Square = %d\n",final_count-final_count1);

}
